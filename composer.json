{
    "name": "bahleel/bahleel",
    "description": "PHP Web Scraping Framework - Build powerful web scrapers with ease",
    "keywords": ["scraping", "web-scraping", "crawler", "spider", "roach-php", "laravel-zero", "cli"],
    "homepage": "https://github.com/bahleel/bahleel",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/bahleel/bahleel/issues",
        "source": "https://github.com/bahleel/bahleel"
    },
    "authors": [
        {
            "name": "Bahleel Team",
            "homepage": "https://github.com/bahleel/bahleel"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/database": "^12.17",
        "laravel-zero/framework": "^12.0.2",
        "roach-php/core": "^3.2"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.25.1",
        "mockery/mockery": "^1.6.12",
        "pestphp/pest": "^3.8.4|^4.1.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Spiders\\": "spiders/",
            "Middlewares\\": "middlewares/",
            "Processors\\": "processors/",
            "Exporters\\": "exporters/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["bahleel"]
}
